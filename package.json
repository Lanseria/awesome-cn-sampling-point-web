{
  "name": "awesome-cn-sampling-point-web",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "fetch": "git clone -b sampling-point https://github.com/Lanseria/awesome-cn-data cafe 2> /dev/null || git -C cafe pull",
    "data": "node ./scripts/process.mjs",
    "postinstall": "npm run fetch && npm run data",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint src",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify/vue": "^3.2.1",
    "@mapbox/mapbox-gl-language": "^1.0.0",
    "@vueuse/core": "^8.3.1",
    "emitt": "^1.3.4",
    "fuse.js": "^6.5.3",
    "mapbox-gl": "^2.8.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.21.1",
    "@iconify-json/carbon": "^1.1.3",
    "@types/node": "^17.0.31",
    "@unocss/reset": "^0.31.17",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "2.0.0-rc.21",
    "eslint": "^8.14.0",
    "jsdom": "^19.0.0",
    "pnpm": "^7.0.0",
    "typescript": "^4.6.4",
    "unocss": "^0.31.17",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.6",
    "vite-plugin-pages": "^0.22.0",
    "vitest": "^0.9.4",
    "vue-tsc": "^0.34.11"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
